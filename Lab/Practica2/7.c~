#include <stdio.h>
#include <string.h>

int main (void)
{
  char Frase [200];
  printf ("A continuacion, introduzca una frase de 3 palabras\n");
  gets (Frase);
  QuitarPalabra (Frase);
}

void QuitarPalabra (char Cadena[])
{
  int i;
  Palabra = 1;
  for (i = 0; i < (strlen(Cadena)); i ++)
    {
      if (Cadena [i] != ' ')
	printf ("%c", &Cadena [i]);
    }
}
